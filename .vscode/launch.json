	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"start"
			],
			"envFile": "${workspaceFolder}/.env",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/server.js",
			"env": {
				"FORGE_CLIENT_ID": "",
				"FORGE_CLIENT_SECRET": "",
				"FORGE_CALLBACK_URL": "",
				"FORGE_ACTIVITYID": "",
				"BASEAPI": "",
				"SERVER_SESSION_SECRET": "",
				"MONGO_CONNECTION_STRING": "",
				"MONGO_DB_NAME": ""
			}
		}
	]
}